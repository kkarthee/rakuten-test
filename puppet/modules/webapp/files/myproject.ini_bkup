uwsgi]
uid = %u
gid = %g
chdir = /webapps/devops/
project = myproject
wsgi-file = app..py
module = wsgi:app
protocol = http
#listen = 400
wsgi-file = app.py
pidfile = /tmp/%(project)-pid
#harakiri = 50
socket = :8000
master = True
#processes = 4
processes = %k
workers = %k
threads = 2
thunder-lock = True
socket-timeout = 65
enable-threads = True
socket = /run/%(project)/%(project).sock
chmod-socket = 660
chown-socket = %(uid):%(gid)
vacuum = true
logto = /var/log/myproject.log
log-maxsize = 10000
cpu-affinity = 1
die-on-term = True
